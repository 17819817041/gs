wd () {\r\n            this.$router.push(\"/forgetPwd\")\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n@import \"@/less/css.less\";\r\n    .reset {\r\n        flex: 10;\r\n        height: 100%;\r\n    }\r\n    .reset_content {\r\n        width: 100%;\r\n        height: 100%;\r\n        .reset_form {\r\n            width: 700px;\r\n        }\r\n    }\r\n    .resetText {\r\n        font-size: 30px;\r\n    }\r\n    .size14a {\r\n        font-size: 14px;\r\n    }\r\n    .forgetPass {\r\n        transform: translate(0,-20%);\r\n    }\r\n    .color {\r\n        color: @hdColor;\r\n    }\r\n    .padding1 {\r\n        padding: 10px 0 35px 0;\r\n    }\r\n</style>"]}]}